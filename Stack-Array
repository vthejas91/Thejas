mport java.util.*;
public class Stack {

    int top;
    int[] array =new int[10];
    public Stack()
    {
        top=-1;
    }
    
    public void push(int num)
    {
        top++;
        array[top]=num;
    }
    public void pop()
    {        
        top--;
    }
    public void display()
    {        
        System.out.println(array[top]);
    }
    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        
   
        
    }
    
}
